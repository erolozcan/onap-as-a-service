{
    "cloud-owner": "${CLOUD_OWNER}",
    "cloud-region-id": "${CLOUD_NAME}",
    "cloud-type": "openstack",
    "owner-defined-type": "",
    "cloud-region-version": "pike",
    "cloud-zone": "",
    "complex-name": "${CLOUD_COMPLEX_NAME}",
    "identity-url": "WillBeUpdatedByMultiCloud",
    "sriov-automation": false,
    "cloud-extra-info": "{\"openstack-region-id\":\"${CLOUD_REGION}\"}",
    "esr-system-info-list": {
    	"esr-system-info": [
            {
            "esr-system-info-id": "${CLOUD_ESR_UUID}",
