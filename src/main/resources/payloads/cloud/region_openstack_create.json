            "password": "${CLOUD_OS_PASSWORD}",
            "system-type": "VIM",
            "ssl-insecure": true,
            "cloud-domain": "${CLOUD_OS_DOMAIN}",
            "default-tenant": "${CLOUD_OS_PROJECT}",
            "system-status": "active"
            }
        ]
    }
}